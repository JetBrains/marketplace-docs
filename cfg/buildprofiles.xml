<?xml version="1.0" encoding="UTF-8"?>

<buildprofiles>
    <variables>
        <images-home>images</images-home>
        <help-app-version>3</help-app-version>
        <stylesheets-home>css</stylesheets-home>
        <web-root>https://plugins.jetbrains.com</web-root>
        <web-community-path>Developer Community:http://www.jetbrains.net/devnet/community/idea/kb</web-community-path>
        <download-page>https://plugins.jetbrains.com/build-and-market</download-page>
        <config-download-title>Start Selling Plugins</config-download-title>
        <images-prefix-override>images/</images-prefix-override>
        <force-resources-app-url>true</force-resources-app-url>
        <config-disqus-id>jetbrains</config-disqus-id>
        <config-disqus-show>false</config-disqus-show>
        <config-feedback-enabled>true</config-feedback-enabled>
        <config-feedback-widget>true</config-feedback-widget>
        <config-feedback-support>mailto:marketplace@jetbrains.com</config-feedback-support>
        <config-feedback-url>https://data.services.jetbrains.com/feedback</config-feedback-url>
        <config-webmaster>marketplace@jetbrains.com</config-webmaster>
        <config-sideblocks>true</config-sideblocks>
        <config-search-scopes-provider>https://www.jetbrains.com/search/json/</config-search-scopes-provider>
        <product-web-url>https://plugins.jetbrains.com</product-web-url>
    </variables>
    <layout name="New Web help" mode="web2">
        <style>s.css</style>
        <views toc="true" search="true" index="true" fav="true"/>
        <xsl>topic.web.new.xsl</xsl>
    </layout>
    <build-profile product="marketplace-docs"/>
</buildprofiles>
